<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lixiaozhuo.androidcomponent">
    <!--用户权限-->
    <!--联网-->
    <uses-permission android:name="android.permission.INTERNET"/>
    <!--网络状态-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--手机联系人-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <application
        android:name=".thread.app.ThreadCustomApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--============================Calculator===============================================-->
        <activity android:name=".calculator.CalculatorActivity"/>
        <!--============================intent===============================================-->
        <activity android:name=".intent.IntentActivity1"/>
        <activity android:name=".intent.IntentActivity2"/>
        <activity
            android:name=".intent.IntentActivity3"
            android:label="隐式打开" >
            <intent-filter>
                <action android:name="com.lixiaozhuo.androidcomponent.intent.ACTION_START" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.lixiaozhuo.androidcomponent.intent.MY_CATEGORY" />
            </intent-filter>
        </activity>
        <activity android:name=".intent.IntentActivity4"/>
        <activity android:name=".intent.IntentActivity5"/>
        <!--=============================Fragment==============================================-->
        <activity android:name=".view.fragment.FragmentActivity"/>
        <!--Fragment2_1-->
        <activity android:name=".view.fragment.fragment1.Fragment1Activity"/>
        <!--Fragment2_2-->
        <activity android:name=".view.fragment.fragment2.Fragment2Activity"/>
        <!--Fragment3-->
        <activity android:name=".view.fragment.fragment3.Fragment3Activity"/>
        <!--==============================Activity=============================================-->
        <activity android:name=".activity.Activity1" />
        <activity android:name=".activity.Activity2" />
        <activity
            android:name=".activity.Activity3"
            android:theme="@android:style/Theme.Dialog" />
        <!--=============================Service==============================================-->
        <activity android:name=".service.ServiceActivity"/>
        <service
            android:name=".service.MyService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".service.NativeMusicService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".service.RemoteMusicService"
            android:enabled="true"
            android:exported="true"/>
        <!--=============================Broadcast==============================================-->
        <activity android:name=".broadcast.BroadcastActivity" />
        <receiver android:name=".broadcast.BroadcastReceiverLife">
            <intent-filter >
                <action android:name="com.lixiaozhuo.androidcomponent.broadcast.BroadcastLife"/>
            </intent-filter>
        </receiver>
        <!--==============================ContentProvider=============================================-->
        <activity android:name=".content_provider.ContentProviderActivity"/>
        <!--provider-->
        <activity android:name=".content_provider.provider.ContentProvider_ProviderActivity"/>
        <provider
            android:name=".content_provider.provider.ContentProvider_Provider"
            android:authorities="com.lixiaozhuo.provider"
            android:enabled="true"
            android:exported="true" />
        <!--user-->
        <activity android:name=".content_provider.user.ContentProvider_UserActivity"/>
        <!--system-book-->
        <activity android:name=".content_provider.system_book.ContentProvider_SystemBookActivity"/>
        <!--==========================View=================================================-->
        <activity android:name=".view.ViewActivity"/>
        <!-- Login-->
        <activity android:name=".view.login.LoginActivity"/>
        <!-- Menu-->
        <activity android:name=".view.menu.MenuActivity"/>
        <!-- Spinner-->
        <activity android:name=".view.spinner.SpinnerActivity"/>
        <!-- Toast-->
        <activity android:name=".view.toast.ToastActivity"/>
        <!-- Dialog-->
        <activity android:name=".view.dialog.DialogActivity"/>
        <activity android:name=".view.dialog.RadiobuttonActivity"/>
        <!-- ListView-->
        <activity android:name=".view.listview.ListViewActivity"/>
        <!-- Notification-->
        <activity android:name=".view.notification.NotificationActivity"/>
        <activity android:name=".view.notification.Notification_ImageActivity"/>
        <service android:name=".view.notification.Notification_MediaService"/>
        <service android:name=".view.notification.Notification_DownloadService"/>
        <!--RecyclerView-->
        <activity android:name=".view.recyclerview.RecyclerViewActivity"/>
        <!--Tab-->
        <activity android:name=".view.tab.TabActivity2"/>
        <activity android:name=".view.tab.MyTabActivity"/>
        <!--AppWidget-->
        <receiver android:name=".view.appwidget.MyAppWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
                <action  android:name="com.lixiaozhuo.androidcomponent.view.appwidget.preview"/>
                <action  android:name="com.lixiaozhuo.androidcomponent.view.appwidget.next"/>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider"
                android:resource="@xml/widget_info" />
        </receiver>
        <!--============================数据存储===============================================-->
        <activity android:name=".save_data.SaveDataActivity"/>
        <!-- SharedPreferences-->
        <activity android:name=".save_data.shared_preferences.SharedPreferencesActivity"/>
        <!-- SQLite-->
        <activity android:name=".save_data.sqlite.SQLiteActivity"/>
        <activity android:name=".save_data.sqlite.SQLiteApplicationActivity"/>
        <!--file-->
        <activity android:name=".save_data.file.FileActivity"/>
        <!--============================Network===============================================-->
        <activity android:name=".network.NetworkActivity"/>
        <!--网络连接-->
        <activity android:name=".network.network_operate.NetworkOperatorActivity"/>
        <!--数据解析-->
        <activity android:name=".network.data_parser.DataParserActivity"/>
        <activity android:name=".network.data_parser.json.JsonActivity"/>
        <activity android:name=".network.data_parser.sax.SaxActivity"/>
        <activity android:name=".network.data_parser.login.NetworkLoginActivity"/>
        <!--JavaScript-->
        <activity android:name=".network.javascript.JavaScriptActivity"/>
        <activity android:name=".network.javascript.webview.WebViewActivity"/>
        <activity android:name=".network.javascript.webview_javascript.WebViewJavaScriptActivity"/>
        <activity android:name=".network.javascript.webview_javascript.WebViewJavaScriptOpenActivity"/>
        <!--============================Thread===============================================-->
        <activity android:name=".thread.ThreadActivity"/>
        <!--Handler-->
        <activity android:name=".thread.handler.HandlerActivity"/>
        <activity android:name=".thread.handler.handler1.Handler1Activity"/>
        <activity android:name=".thread.handler.handler2.Handler2Activity"/>
        <activity android:name=".thread.handler.handler3.Handler3Activity"/>
        <activity android:name=".thread.handler.handler_app.HandlerApplicationActivity"/>
        <!--AsyncTask-->
        <activity android:name=".thread.asynctask.AsyncTaskActivity"/>
        <activity android:name=".thread.asynctask.asynctask1.AsyncTask1Activity"/>
        <activity android:name=".thread.asynctask.asynctask2.AsyncTask2Activity"/>
        <activity android:name=".thread.asynctask.asynctask_app.AsyncTaskApplicationActivity"/>
        <!--线程通信-->
        <activity android:name=".thread.communication.CommunicationActivity"/>
        <!--应用-->
        <activity android:name=".thread.app.ThreadApplicationActivity1"/>
        <activity android:name=".thread.app.ThreadApplicationActivity2"/>
    </application>
</manifest>